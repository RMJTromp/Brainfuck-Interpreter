{
    "name": "brainfuck-interpreter",
    "description": "A BrainFuck interpreter, text encoder, and debugger/visualizing tool for BrainFuck",
    "version": "1.0.0",
    "dependencies": {
        "svg-tag-names": "^3.0.1"
    },
    "homepage": "https://brainfuck.rmjtromp.com",
    "repository": {
        "url": "git+https://github.com/RMJTromp/Brainfuck-Interpreter.git",
        "type": "git"
    },
    "author": {
        "name": "RMJTromp",
        "url": "https://rmjtromp.com"
    },
    "bugs": {
        "url": "https://github.com/RMJTromp/Brainfuck-Interpreter/issues"
    },
    "scripts": {
        "build": "parcel build src/index.html --public-url /",
        "dev": "parcel serve src/index.html --public-url /"
    },
    "alias": {
        "dom-chef": "/src/js/plugins/dom-chef@5.1.0"
    },
    "browserslist": [
        "> 0.5%, last 2 versions, not dead"
    ],
    "targets": {
        "browser": {
            "context": "browser",
            "outputFormat": "commonjs",
            "includeNodeModules": [
                "svg-tag-names"
            ],
            "isLibrary": false,
            "sourceMap": true,
            "optimize": true
        }
    },
    "devDependencies": {
        "parcel": "^2.9.1",
        "postcss": "^8.4.24",
        "postcss-nested": "^6.0.1",
        "postcss-url": "^10.1.3",
        "typescript": "^5.0.4"
    }
}

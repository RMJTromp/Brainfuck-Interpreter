{
    "name": "brainfuck-interpreter",
    "description": "A BrainFuck interpreter, text encoder, and debugger/visualizing tool for BrainFuck",
    "version": "1.0.0",
    "dependencies": {
        "svg-tag-names": "^3.0.1"
    },
    "homepage": "https://brainfuck.rmjtromp.com",
    "repository": {
        "url": "git+https://github.com/RMJTromp/Brainfuck-Interpreter.git",
        "type": "git"
    },
    "author": {
        "name": "RMJTromp",
        "url": "https://rmjtromp.com"
    },
    "bugs": {
        "url": "https://github.com/RMJTromp/Brainfuck-Interpreter/issues"
    },
    "scripts": {
        "build": "parcel build src/index.html src/learn.html --public-url /",
        "dev": "parcel serve src/index.html src/learn.html --public-url /"
    },
    "alias": {
        "dom-chef": "/src/js/plugins/dom-chef@5.1.0"
    },
    "browserslist": [
        "> 0.5%, last 2 versions, not dead"
    ],
    "targets": {
        "default": {
            "context": "browser",
            "outputFormat": "commonjs",
            "includeNodeModules": [
                "svg-tag-names",
                "dom-chef"
            ],
            "isLibrary": false,
            "sourceMap": true,
            "optimize": true
        }
    },
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.16.4",
        "@parcel/transformer-jsonld": "^2.16.4",
        "@parcel/transformer-webmanifest": "^2.16.4",
        "parcel": "^2.16.4",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "postcss": "^8.5.6",
        "postcss-nested": "^7.0.2",
        "postcss-url": "^10.1.3",
        "typescript": "^5.9.3"
    },
    "engines": {
        "npm": "11.x",
        "node": "24.x"
    }
}
